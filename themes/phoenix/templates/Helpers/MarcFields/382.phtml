<? 
if (!empty($data['subfieldData'])) {
    foreach($data['subfieldData'] as $key => $value) {
        if (substr($key, 0, 1) == 'a') {
            echo ' |    ' . $value; 
        }
        elseif (substr($key, 0, 1) == 'b') {
            echo ' | soloist:' . $value;
        }
        elseif (substr($key, 0, 1) == 'd') {
            echo '/' . $value;
        }
        elseif (substr($key, 0, 1) == 'e') {
            if ($value != '1') {
                echo '(' . $value . ')';
            }
        }
        elseif (substr($key, 0, 1) == 'n') {
            if ($value != '1') {
                echo '(' . $value . ')';
            }
        }
        elseif (substr($key, 0, 1) == 'p') {
            echo ' or ' . $value;
        }
        elseif (substr($key, 0, 1) == 'v') {
            echo ' [' . $value . ']';
        }
    }
    echo '<br/>'; 
}
?>   

<? if (!empty($data['vernacularData'])) :
       echo htmlentities(implode(' ', $data['vernacularData']));?><br />
<? endif; ?>   
